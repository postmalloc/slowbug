{
  "name": "slowbug",
  "publisher": "srimukh",
  "displayName": "Slowbug - Watch your code run, slowly",
  "description": "Slowbug is a VS Code extension for debugging your code in slow-mo.",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Debuggers",
    "Other"
  ],
  "keywords": [
    "debug",
    "controls",
    "visualization",
    "slow"
  ],
  "activationEvents": [
    "onCommand:extension.slowbug-start",
    "onCommand:extension.slowbug-pause"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.slowbug-start",
        "title": "Start Slowbug",
        "category": "Slowbug",
        "icon": {
          "dark": "images/slow_icon_dark.png",
          "light": "images/slow_icon_light.png"
        }
      },
      {
        "command": "extension.slowbug-pause",
        "title": "Pause Slowbug",
        "category": "Slowbug",
        "icon": {
          "dark": "images/pause_icon_dark.png",
          "light": "images/pause_icon_light.png"
        }
      }
    ],
    "menus": {
      "editor/title": [
        {
         "when": "true",
         "command": "extension.slowbug-start",
         "group": "navigation@420"
        },
        {
          "when": "true",
          "command": "extension.slowbug-pause",
          "group": "navigation@421"
         }
      ]
    },
    "configuration": {
      "title": "Slowbug",
      "properties": {
        "slowbug.stepDuration": {
          "type": "integer",
          "default": 800,
          "description": "Duration of each step (in milliseconds)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1"
  },
  "repository": "https://github.com/postmalloc/slowbug.git"
}
